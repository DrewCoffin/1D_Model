set terminal jpeg
set key below
set size 1, 1
set xlabel 'System III Longitude'
set ylabel 'DENS'
set xrange [0:360]
set xtics 45
set grid ytics
set yrange [0:300]
set title 'Temporal Variability of Flux Tube Content (001)'
set output 'gplot001.jpeg'
plot 'DENSsp0001.dat'  with lines title 'Sulfur + (II)',   \
     'DENSs3p0001.dat'  with lines title 'Sulfur +++ (IV)',    \
     'LOAD0001.dat'  with lines title 'Normalized Loading'   
